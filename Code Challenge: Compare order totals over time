-- SQL request(s)​​​​​​‌​‌‌‌​​‌​​‌​​​​‌​​‌​‌​‌​‌ below
SELECT
    o.ORDERID,
    sum(d.PRICE) as thisorderprice,
    sum(d.PRICE) - lag(sum(d.PRICE)) over(order by o.ORDERDATE) as DiffFromPrev
FROM orders o
JOIN ordersdishes od on o.ORDERID = od.ORDERID
JOIN dishes d on d.dishid = od.dishid
WHERE OrderDate >= '2022-01-01'
GROUP BY o.ORDERID

